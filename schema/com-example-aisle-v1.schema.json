{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "href": "https://example.com/schemas/com-example-aisle-v1",
  "title": "Aisle v1 Schema",
  "description": "An individual aisle within a store.",
  "type": "object",
  "anyOf": [ { "$ref": "https://example.com/schemas/com-example-base-v1" } ],
  "properties": {
    "storeId": {
      "type": "string",
      "description": "Hyperlink to the store where this aisle is located."
    },
    "aisleName": {
      "type": "string",
      "description": "Aisle name. Examples: `Baking` and `Dairy`"
    },
    "aisleNumber": {
      "type": "integer",
      "description": "Aisle number. Examples: `1` and `2`"
    },
  },
  "required": [ "storeId", "aisleName" ]
}
